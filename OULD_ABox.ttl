    ould:hasUpdatedEvent ould:Event2 ;
    ould:hasUpdatedValue ould:Wall1 ;
    ould:hasIFCModelID "ifc:Wall2" ;
    ould:hasHash "xyz789" .

ould:Wall2 a ould:UpdatableEntity ;
